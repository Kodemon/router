{
  "name": "router",
  "description": "A react router hook layer for https://github.com/Kodemon/router",
  "version": "1.0.7",
  "private": true,
  "author": "Christoffer Rodvik <christoffer@kodemon.net>",
  "license": "MIT",
  "repository": "cmdo/router",
  "keywords": [
    "router",
    "web",
    "spa",
    "react"
  ],
  "scripts": {
    "setup": "yarn setup:router && yarn setup:native && yarn setup:react && yarn build && yarn bootstrap",
    "setup:router": "cd ./packages/router && yarn",
    "setup:native": "cd ./packages/native && yarn",
    "setup:react": "cd ./packages/react && yarn",
    "build": "yarn build:router && yarn build:native && yarn build:react",
    "build:router": "cd ./packages/router && yarn build",
    "build:native": "cd ./packages/native && yarn build",
    "build:react": "cd ./packages/react && yarn build",
    "demo:native": "cd ./demo/native && yarn && expo start",
    "demo:web": "cd ./demo/web && yarn && yarn start",
    "test": "yarn test:router && yarn test:react",
    "test:router": "cd ./packages/router && npm test",
    "test:react": "cd ./packages/react && npm test",
    "publish": "yarn publish:router && yarn publish:native && yarn publish:react",
    "publish:router": "cd ./packages/router && npm publish",
    "publish:native": "cd ./packages/native && npm publish",
    "publish:react": "cd ./packages/react && npm publish",
    "bootstrap": "node bootstrap.js"
  }
}
